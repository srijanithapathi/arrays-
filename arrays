#include<stdio.h>
int main()
{
	int a[3][3],i,j,sum;
	printf("Enter the elements of array :");
	for(i=0;i<3;++i)
	{
			for(j=0;j<3;++j)
			{
			 scanf("%d",&a[i][j]);	
			}

	}
	
	printf("The array elements are :");
	{
	for(i=0;i<3;++i)
	{
			for(j=0;j<3;++j)
			{
			 printf("%d",a[i][j]);
			}
		if(j==3)
		printf("\n");
	}
	
}
for(i=0;i<3;++i)
sum+=a[i][i];

printf("The sum of diagonal elements is: %d ",sum);

}
